---
import Layout from "../../layouts/Layout.astro";
import { MainLayout } from "../../layouts/MainLayout";
import { DashboardView } from "../../components/inventario/Tablero";
import { INITIAL_PRODUCTS } from "../../data/mocks";

const currentPath = Astro.url.pathname;
---

<Layout title="Tablero Principal - ERP">
	<MainLayout currentPath={currentPath} client:load>
		<DashboardView products={INITIAL_PRODUCTS} client:load />
	</MainLayout>
</Layout>
